{
  "extends": "airbnb/legacy",
  "rules": {
    "comma-dangle": [2, "never"],
    "space-before-function-paren": [2, "never"],
    "max-len": [2, 120, 4],
    "no-eval": [0],
    "quote-props": [2, "always"],
    "func-names": [0],
    "vars-on-top": [0],
    "no-param-reassign": [0],
    "one-var": [0],
    "no-cond-assign": [2, "except-parens"],
    "dot-notation": [0]
  }
}
